{
    "name": "makinacorpus/cron",
    "type": "library",
    "description": "Simple applicative cron API",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Makina Corpus",
            "email": "drupal@makina-corpus.com",
            "homepage": "https://makina-corpus.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Cron\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakinaCorpus\\Cron\\Tests\\": "tests/"
        }
    },
    "require": {
        "makinacorpus/goat-query": ">= 3",
        "php": ">=8.0",
        "psr/log" : "^1 || ^2",
        "ramsey/uuid": ">= 3.8",
        "makinacorpus/argument-resolver": "^1.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/config": "^5.4|^6.0",
        "symfony/console": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0"
    }
}
